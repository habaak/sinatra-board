    <div class="text-center my-3">
        <h1>회원 목록 입니다.</h1>
    </div>
    <div class="text-right my-3">
        <a class="btn btn-primary " href="/posts/new" role="button">글 쓰러가기</a>
    
    </div>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">id</th>
          <th scope="col">이메일</th>
          <th scope="col">이름</th>
          <th scope="col">비밀번호</th>
          <th scope="col">가입일자</th>
          <th scope="col">보기</th>
        </tr>
      </thead>
      <tbody>
          <% @user.each do |u| %>
          
        <tr>
          <th scope="row"><%= u.id%></th>
          <td><%= u.email%></td>
          <td><%= u.name%></td>
          <td><%= u.password%></td>
          <td><%= u.created_at%></td>
          <td><a class="btn btn-primary " href="/users/details/<%=u.id%>" role="button">보기</a></td>
        </tr>
        <% end %>
      </tbody>
    </table>